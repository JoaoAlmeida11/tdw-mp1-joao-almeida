{
  "root": true,
  "env": {
      "browser": true,
      "es2021": true,
      "node": true,
      "jest": true
  },
  "extends": [
      "plugin:gatsby/recommended"
  ],
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "plugins": [
      "gatsby",
      "prettier"
  ],
  "globals": {
    "graphql": true
  },
  "settings": {
      "react": {
          "version": "detect"
      }
  },
  "rules": {
    
  }
}
